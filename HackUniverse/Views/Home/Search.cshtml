@model List<Hackathon>
@{
    ViewData["Title"] = "Search";
}

@if (Model.Count != 0)
            {
<br />
<br />

<h6>Your Hackathons</h6>
<div class="card-columns">

    @foreach (var item in Model)
    {

        <a asp-controller="Hackathon" asp-action="Index" asp-route-hid="@item.Id">
            <div class="card mx-auto">
                <img class="card-img" src='~/Images/Hackathons/thumb@(item.Id).png' style="height:10em;width:10em" />
                <div class="card-body">

                    <h5 class="card-title">@item.Title</h5>
                </div>

            </div>
        </a>


    }

</div>
            }

